// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Client {
  id String @id
  name String
  email String @unique
  password String 
  cpf String @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
} 

model Product {
  id String @id
  name String
  description String
  price Int
  quantity Int
}